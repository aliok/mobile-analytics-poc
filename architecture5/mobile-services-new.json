        {
          "alias": "",
          "expr": "count (count by (clientId)(sdkInitEvent))",
          "format": "table",
          "hide": false,
          "intervalFactor": 1,
          "rawSql": "SELECT\n  count(distinct latestEntryInTimeRange.clientId)\nFROM\n    (select entry.clientId, entry.event_time, entry.data \n      from mobileAppMetrics entry \n      inner join \n        (select clientId, max(event_time) as latestEntryTime \n          from mobileAppMetrics \n          where $__timeFilter(event_time) and event_type='init'\n          group by clientId\n        ) \n        latestEntry \n        on entry.clientId = latestEntry.clientId and entry.event_time = latestEntryTime\n        WHERE event_type='init'\n      ) latestEntryInTimeRange;",
          "refId": "A"
        }
      ],
